name=input("Enter your name:")
roll=int(input("Enter your roll number:"))
eng=int(input("Enter your english marks :"))
maths=int(input("Enter your maths marks:"))
pys=int(input("Enter your pys marks:"))
chem=int(input("Enter your chem marks:"))
isl=int(input("Enter your isl marks:"))
obtain=eng+maths+pys+chem+isl
total=500
per= (obtain/total)*100

if per >= 70:
    print(f"Dear student {name}\nroll number {roll} \nobtain marks {obtain} \nyou got {per} \nGrade: A")
elif per >= 60:
    print(f"Dear student {name}\nroll number {roll} \nobtain marks {obtain} \nyou got {per} \nYour Grade: B")
elif per >= 50:
    print(f"Dear student{name}\nroll number {roll} \nobtain marks  {obtain} \nyou got {per} \nYour Grade: C")
elif per >= 40:
    print(f"Dear student {name}\nroll number {roll} \nobtain marks {obtain} \nyou got {per} \nYour Grade: D")
else:
    print("Grade: Fail")